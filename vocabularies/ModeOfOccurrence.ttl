    skos:inScheme cs: ;
    skos:narrower :interstitial-material ;
    skos:prefLabel "cement"@en ;
    skos:topConceptOf cs: ;
.

:corona
    a skos:Concept ;
    skos:broader :mineral-overgrowth ;
